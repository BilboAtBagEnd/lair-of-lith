<% @title = "#{@character.name} by #{@owner.name}" %>
<h2><%= @character.name %></h2>
<p>by <%= @owner.name %></p>

<h3>Versions</h3>
<ul>
<% @versions.each do |version| %>
  <li>
    <%= link_to "Version #{version.version}", 
    controller: 'characters', action: 'edit', id: @character.id, vid: version.id %>
    <%= version.created_at %>
  </li>
<% end %>
</ul>
